.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}

.songlist-item{
  position: relative;
  padding-top: 5px;
  display: flex;
  width:100%
 
}
.songlist-image{
  width: 80px;
  height: 80px
}
.songlist-image image{
  width: 100%;
  height: 100%;
}
.songlist-info{
  padding-left: 10px;
  flex: 1;
}
.songlist-info view{
  padding-bottom: 10px;
}
.songlist-info .label{
  margin-right: 5px;
  color: #666;
}
.songlist-info .rating{
  color: #e09015;
}
.songlist-title{
 font-size: 18px;
 margin-right: 10px;
}
.songlist-num{
  padding-top: 20px;
  font-size: 16px;
  color: #999;
}

.song-item{
  position: relative;
  padding-top: 5px;
  display: flex;
}
.song-image{
  width: 80px;
  height: 80px
}
.song-image image{
  width: 100%;
  height: 100%;
}
.song-info{
  padding-left: 10px;
  flex: 1;
  border-bottom: 1px #ddd solid
}
.song-info view{
  padding-bottom: 10px;
}
.song-info .label{
  margin-right: 5px;
  color: #666;
}
.song-info .rating{
  color: #e09015;
}
.song-title{
 font-size: 18px;
 margin-right: 10px;
}
.song-singer{
  padding-top: 20px;
  font-size: 16px;
  color: #999;
}
.person{
  margin-right: 5px;
}